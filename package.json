{
  "name": "@papijs/papi",
  "version": "0.4.0",
  "description": "A simple RESTful API interface",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "bundlesize": [
    {
      "path": "./dist/main.js",
      "maxSize": "3 kB"
    }
  ],
  "nyc": {
    "exclude": [
      "dist",
      "test"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --colors -w --config webpack.dev.js",
    "publish": "npm publish --access=public",
    "test": "nyc mocha --exit && nyc report --reporter=text-lcov | coveralls && npm run test:bundlesize",
    "test:default": "NODE_ENV=test npm run build && nyc mocha --exit",
    "test:coverage": "nyc --reporter=lcov  --reporter=text-lcov npm run test:default",
    "test:watch": "mocha -w",
    "test:bundlesize": "npm run build && bundlesize",
    "pretest": "NODE_ENV=test npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papijs/papi.git"
  },
  "keywords": [
    "node",
    "rest",
    "api"
  ],
  "author": "Andrew Farinella",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/papijs/papi/issues"
  },
  "homepage": "https://github.com/papijs/papi#readme",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@types/async": "^3.0.1",
    "@types/chai": "^4.2.2",
    "@types/expect": "^1.20.4",
    "@types/jest": "^24.0.16",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/proxyquire": "^1.3.28",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^7.0.13",
    "babel-loader": "^8.0.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.0.0",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "istanbul": "^0.4.5",
    "jest": "^24.8.0",
    "mocha": "^5.2.0",
    "nodemon-webpack-plugin": "^4.0.6",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.3",
    "rewire": "^4.0.1",
    "sequelize-test-helpers": "^1.1.2",
    "sinon": "^4.5.0",
    "supertest": "^3.1.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
