{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10.13.0"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@": "./"
        }
      }
    ]
  ]
}
